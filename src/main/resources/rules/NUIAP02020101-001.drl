package com.springcloud.blog.admin.drools
import com.gu.drools.fosunhealth.entity.*
dialect  "java"

rule "NPIAC010106-002"
    when
    $result:ResultDto()
    险种 : Lcpol()
    eval(险种.getRiskCode().equals("111"))
    then
    $result.addVerify("被保人国籍中国香港（HKG-中国香港）、中国澳门（MAC-澳门）、中国台湾（TWN-中国台湾），累计重疾险基本保额最高限制100万",drools.getRule().getName());
end